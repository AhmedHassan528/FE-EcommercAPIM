{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.css",
            "*.js",
            "*.png",
            "*.jpg",
            "*.jpeg",
            "*.gif",
            "*.svg",
            "*.ico",
            "/assets/*"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    },
    "globalHeaders": {
        "Cache-Control": "no-cache"
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}